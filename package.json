    "on-build-webpack": "^0.1.0",
    "tar": "^6.0.5",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "yargs": "^16.2.0"
  },
